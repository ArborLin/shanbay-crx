{
    "name": "扇贝助手增强版",
    "version": "1.7.1",
    "manifest_version": 2,
    "description": "任意网页查询并加入生词本、背单词页面简洁、增强词根词缀Webster功能、增加实用快捷键",
   "icons": {
      "48": "icon_48.png",
      "72": "icon_72.png"
   },
    "background": {
        "scripts": ["js/jquery-2.0.3.min.js","js/background.js"]
    }, 
    "options_page":"options.html",
    "content_scripts": [
        {
            "matches": ["http://*.shanbay.com/review**"],
            "css": ["style.css"],
            "js": ["js/jquery-2.0.3.min.js", "js/etymology.js","js/webster.js","js/main.js"]
        },{
      "css": [ "shanbay.css" ],
      "js": [ "js/jquery-2.0.3.min.js", "js/howler.min.js", "js/shanbay.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   }
    ],
    "permissions": [
       "cookies","contextMenus", "tabs","storage", "http://*/*", "https://*/*"
    ],
    "web_accessible_resources": []
}
